{
  "name": "proseline-protocol",
  "description": "proseline.com replication protocol",
  "version": "7.0.0",
  "author": "Kyle E. Mitchell <kyle@kemitchell.com> (https://kemitchell.com/)",
  "dependencies": {
    "fast-json-stable-stringify": "^2.0.0",
    "json-protocol": "^2.0.2",
    "sodium-universal": "^2.0.0",
    "strict-json-object-schema": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "husky": "^0.14.3",
    "licensee": "^7.0.1",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "tape": "^4.9.1"
  },
  "keywords": [
    "peer-to-peer",
    "proseline",
    "protocols"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "repository": "proseline/proseline-protocol.js",
  "scripts": {
    "licenses": "licensee --errors-only",
    "lint": "eslint '**/*.js'",
    "precommit": "lint-staged",
    "format": "prettier --write '**/*.{js,json,md}'",
    "test": "node test.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
